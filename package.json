{
	"name": "affiliate-frontend",
	"version": "1.0.0",
	"description": "affiliateed programm",
	"repository": {
		"type": "git",
		"url": "https://bitbucket.ufs-online.ru/projects/AGENCY/repos/affiliate-frontend/browse"
	},
	"main": "client.js",
	"scripts": {
		"webpack-dev-server": "webpack-dev-server --mode development --progress --inline --config webpack.client.config --colors",
		"webpack": "webpack",
		"wds": "set WATCH=true && npm run webpack-dev-server",
		"clear-build": "del-cli --force build/*.*",
		"run-server": "node .\\build\\server.js",
		"sls": "npm run build-local && cd ./build && node server.js",
		"build-server": "webpack --mode development --config webpack.server.config",
		"build-development-mode-client": "npm run clear-build && npm run webpack -- --config webpack.client.config.js --mode development",
		"build-development-mode-server": "npm run webpack -- --config webpack.server.config.js --mode development",
		"build-development-mode": "npm run build-development-mode-client && npm run build-development-mode-server",
		"build-production-mode-client": "npm run clear-build && npm run webpack -- --config webpack.client.config.js --mode production",
		"build-production-mode-server": "npm run webpack -- --config webpack.server.config.js --mode production",
		"build-production-mode": "npm run build-production-mode-client && npm run build-production-mode-server",
		"build-local-client": "export STAND=local|| set STAND=local&& npm run build-development-mode-client",
		"build-local": "export STAND=local|| set STAND=local&& npm run build-development-mode",
		"build-develop": "export STAND=develop|| set STAND=develop&& npm run build-development-mode",
		"build-test": "export STAND=test|| set STAND=test&& npm run build-development-mode",
		"build-production": "export STAND=production|| set STAND=production&& npm run build-production-mode",
		"build-replica": "export STAND=replica|| set STAND=replica&& npm run build-development-mode",
		"deploy-develop": "npm run build-develop && sh ./deploy/deploy.sh cspa-affiliate-develop01 ./build/",
		"deploy-test": "npm run build-test && sh ./deploy/deploy.sh cspa-affiliate-test01 ./build/",
		"deploy-production": "npm run build-production && sh ./deploy/deploy.sh cspa-affiliate.ufs-online.ru ./build/",
		"deploy-replica": "npm run build-replica && sh ./deploy/deploy.sh cspa-affiliate-replica ./build/",
		"clean-develop": "sh ./deploy/cleaner.sh cspa-affiliate-develop01 3",
		"clean-test": "sh ./deploy/cleaner.sh cspa-affiliate-test01 3",
		"clean-production": "sh ./deploy/cleaner.sh cspa-affiliate.ufs-online.ru 3",
		"clean-replica": "sh ./deploy/cleaner.sh cspa-affiliate-replica 3",
		"eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
		"eslint:dump": "eslint --print-config ./.eslintrc"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-array-includes": "^2.0.3",
		"babel-plugin-prismjs": "^1.0.2",
		"babel-plugin-transform-object-assign": "^6.22.0",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"express-handlebars": "^3.0.0",
		"node-sass": "^5.0.0",
		"react": "^16.9.0",
		"react-cookies": "^0.1.0",
		"react-dom": "^16.9.0",
		"react-redux": "^7.1.0",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-scroll": "^1.8.0",
		"redux": "^4.0.4",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.1",
		"@types/express-handlebars": "0.0.33",
		"@types/react": "^16.8.22",
		"@types/react-dom": "^16.8.4",
		"@types/react-redux": "^7.1.1",
		"@types/react-router-dom": "^4.3.4",
		"@types/redux": "^3.6.0",
		"@types/redux-logger": "^3.0.7",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"assets-webpack-plugin": "^3.9.10",
		"autoprefixer": "^9.5.1",
		"copy-webpack-plugin": "^6.3.0",
		"css-loader": "^5.0.1",
		"del-cli": "^3.0.1",
		"file-loader": "^2.0.0",
		"mini-css-extract-plugin": "^1.3.1",
		"modernizr": "^3.7.1",
		"postcss": "^7.0.16",
		"postcss-loader": "^4.0.4",
		"prettier": "^1.19.1",
		"sass-loader": "^10.1.0",
		"terser-webpack-plugin": "^2.1.0",
		"ts-loader": "^6.0.4",
		"typescript": "^3.5.3",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.4.1",
		"webpack-modernizr-loader": "^4.0.1"
	}
}
