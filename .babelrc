{
	"presets": ["@babel/preset-react", "@babel/preset-env"],
	"plugins": [
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		],
		["transform-object-assign"],
		["array-includes"],
		[
			"prismjs",
			{
				"languages": ["javascript", "css", "markup"],
				"plugins": ["line-numbers", "autolinker"]
			}
		]
	]
}
